{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-fct-prod-git"
		},
		"AZ_Blob_Str_Prod_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AZ_Blob_Str_Prod'"
		},
		"AZ_sqldb_Dest_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AZ_sqldb_Dest'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-fct-prod-git_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AZ_Blob_Str_Prod_connectionString": {
						"value": "[parameters('AZ_Blob_Str_Prod_connectionString')]"
					},
					"AZ_sqldb_Dest_connectionString": {
						"value": "[parameters('AZ_sqldb_Dest_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}